<div class="container p-2">
  <%= simple_form_for([@user, @couch]) do |f| %>
    <%= f.input :title %>
    <%= f.input :address %>
    <%= f.input :price %>
    <%= f.input :description %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit "Add a couch", class:"btn btn-flat" %>
  <% end %>
</div>
